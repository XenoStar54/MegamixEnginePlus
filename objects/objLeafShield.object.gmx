if(!global.frozen)
{
    if(canDamage)
    {
        // play the classic annoying sound
        leafSoundTimer++;
        if(leafSoundTimer % 8 == 0)
        {
            playSFX(sfxLeafShield);
        }
        
        if (!instance_exists(parent))
        {
            if (isShield)
            {
                event_user(10);
                exit;
            }
        }
        else
        {
            if (entityCanStep())
            {
                if (isShield)
                {
                    x = spriteGetXCenterObject(parent);
                    y = spriteGetYCenterObject(parent);
                    
                    if(sprite_index == sprLeafShield &amp;&amp; parent.object_index == objMegaman)//Right side is for copy bot.
                    {
                        with(parent)
                        {
                            if (global.keyUp[playerID])
                            {   
                                other.isShield = false;
                                other.yspeed = image_yscale * -4;
                                global.ammo[playerID, global.weapon[playerID]] = max(0,
                                global.ammo[playerID, global.weapon[playerID]] - 3 / (global.energySaver + 1));
                            }
                            else if (global.keyDown[playerID])
                            {   
                                other.isShield = false;
                                other.yspeed = image_yscale * 4;
                                global.ammo[playerID, global.weapon[playerID]] = max(0,
                                global.ammo[playerID, global.weapon[playerID]] - 3 / (global.energySaver + 1));
                            }
                            else if (global.keyLeft[playerID])
                            {   
                                other.isShield = false;
                                other.xspeed = image_xscale * 4;
                                global.ammo[playerID, global.weapon[playerID]] = max(0,
                                global.ammo[playerID, global.weapon[playerID]] - 3 / (global.energySaver + 1));
                            }
                            else if (global.keyRight[playerID] || isSlide)
                            {   
                                other.isShield = false;
                                other.xspeed = image_xscale * 4;
                                global.ammo[playerID, global.weapon[playerID]] = max(0,
                                global.ammo[playerID, global.weapon[playerID]] - 3 / (global.energySaver + 1));
                            }
                        }
                    }
                }
            }
        }
    }
    else
    {
        if (x &gt;= deathPos + 16)
        {
            deadLeafMove = -1.5;
        }
        else if (x &lt;= deathPos - 16)
        {
            deadLeafMove = 1.5;
        }
        
        xspeed = deadLeafMove;
    }
}
</string>
