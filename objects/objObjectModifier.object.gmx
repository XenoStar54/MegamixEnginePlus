            <kind>1</kind>
            <string>if(!global.frozen &amp;&amp; !dead)
{
    // monitor objects
    with(objectToModify) if(!dead)
    {
        var index = 0;
        var foundNum = -1;
        while(index &lt; array_length_1d(other.uniqueObjectIDtracker))
