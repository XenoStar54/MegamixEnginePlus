            other.uniqueIsTouching[index] = 0;
            foundNum = index;
        }
        
        if(place_meeting(x,y,other)) // an object with correct object id contacted me
        {
            if(!other.uniqueIsTouching[foundNum]) // for the first time, modify them
            {
                scriptExecuteNargs(other.script,other.scriptArgs);
                stringExecutePartial(other.code);
                with(other)
                {
                    if(oneTimeUse &gt; 2) instance_destroy();
                    else if(oneTimeUse &gt; 0) dead = 1;
                }
            }
            other.uniqueIsTouching[foundNum] = 1;
        }
        else other.uniqueIsTouching[foundNum] = 0; // that object is no longer contacting me
    }
}
else
{
