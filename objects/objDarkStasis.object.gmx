          <argument>
            <kind>1</kind>
            <string>event_inherited();

if(entityCanStep())
{
    if(!isStasis) // bullet mode; inherit player direction
    {
        if(instance_exists(parent)) // yea whatever
        {
            if(parent.xDir &gt; 0 &amp;&amp; parent.yDir == 0)
            {
                dir = 0;
            }
            else if(parent.xDir &gt; 0 &amp;&amp; parent.yDir &lt; 0)
            {
                dir = 45;
            }
            else if(parent.xDir == 0 &amp;&amp; parent.yDir &lt; 0)
            {
                dir = 90;
            }
            else if(parent.xDir &lt; 0 &amp;&amp; parent.yDir &lt; 0)
            {
                dir = 135;
            }
            else if(parent.xDir &lt; 0 &amp;&amp; parent.yDir == 0)
            {
                dir = 180;
            }
            else if(parent.xDir &lt; 0 &amp;&amp; parent.yDir &gt; 0)
            {
                dir = 225;
            }
            else if(parent.xDir == 0 &amp;&amp; parent.yDir &gt; 0)
            {
                dir = 270;
            }
            else if(parent.xDir &gt; 0 &amp;&amp; parent.yDir &gt; 0)
            {
                dir = 315;
            }
        }
        xspeed = cos(degtorad(dir))*spd;
        yspeed = -sin(degtorad(dir))*spd;
    }
    else // stasis mode; stun enemies within you and damage them every 5 frames
    {
        xspeed = 0;
        yspeed = 0;
        if(timer mod 10 == 0) canDamage = true;
        else canDamage = false;
        with(prtEntity)with(other)
        {
            if(place_meeting(x,y,other) &amp;&amp; global.factionStance[faction,other.faction] &amp;&amp; other.canHit &amp;&amp; !other.dead)
            {
                with(other)
                {
                    if(iceTimer &lt;= 0) entityIceFreeze(2, false, false, 1);
                    else iceTimer = 2;
                }
            }
        }
        if(timer &lt;= 5 &amp;&amp; timer &gt; 0) stasisWallDist -= (20/5);
        else if(timer &lt;= 0) instance_destroy();
        else if(stasisWallDist &lt; 20) stasisWallDist += (20/5);
        timer--;
    }
}
</string>
