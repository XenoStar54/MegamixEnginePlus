    if(state == 0) // fly
    {
        xspeed = spd*image_xscale;
        image_index = 0;
        if(sign(xcoll) == image_xscale)
        {
            xspeed = 0;
            state = 1;
            blastTimer = -7;
            image_index = 1;
            blockCollision = 0;
            playSFX(sfxCrashBombArm);
        }
    }
    else if(state == 1) // I'm on a wall
    {
        xspeed = 0;
        blastTimer++;
        if(blastTimer &lt; 0)
        {
            image_index = 1;
        }
        else
        {
            if(blastTimer mod 7 == 0)
            {
                if(image_index &lt; 3) image_index++;
                else image_index = 2;
            }
            
            if(blastTimer &gt;= 59)
            {
                event_user(0);
            }
        }
    }
    else if(state == 2) // blow up
    {
        xspeed = 0;
        blastTimer++;
        if(blastTimer mod 3 == 0)
        {
            if(image_index &lt; image_number - 1) image_index++;
            else instance_destroy();
        }
        if(blastTimer mod 8 == 0)
        {
            playSFX(sfxEnemyHit);
        }
    }
}
