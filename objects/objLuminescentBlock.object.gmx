            <string>event_inherited();

if (!global.frozen &amp;&amp; !dead &amp;&amp; !global.timeStopped)
{
    var keepGlowing = false;
    
    for(var i = 0; i &lt; array_length_1d(glowSource); i++)
    {
        with(glowSource[i]) if(!dead &amp;&amp; image_alpha &gt;= other.alphaTolerance)
        {
            with(other)
            {
                if(flagStartGlowing != 1)
                {
                    flagStartGlowing = 1;
                    glowState = skipInvis;
                }
                keepGlowing = true;
            }
        }
    }
    
    // handle glowing
    if (flagStartGlowing == 1)
    {
        visible = true;
        glowState += glowSpeed;
        image_index = floor(glowState);
        if (glowState &gt;= maxGlowState) // if the glow is finished, stop glowing or loop
        {
            glowState = (skipInvis * keepGlowing);
            flagStartGlowing = glowState;
        }
    }
    else
    {
        glowState = 0;
        image_index = 0;
        visible = false;
    }
    
    // glimmer slightly in light to prevent trolling
    if(glowState == 0 &amp;&amp; flagStartGlowing == 0)
    {
        glimmerTimer++;
        if(glimmerTimer &gt;= glimmerTimerMax)
        {
            glimmerTimer = 0;
            glimmerTimerMax = 30+irandom_range(0,5)*15;
            var twinkle = instance_create(irandom_range(bbox_left,bbox_right),irandom_range(bbox_top,bbox_bottom),objSingleLoopEffect);
            twinkle.image_speed = 0.3;
        }
    }
    else
    {
        glimmerTimer = 0;
    }
}
</string>
