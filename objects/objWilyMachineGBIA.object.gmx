if (!global.frozen)
{
    if (startIntro)
    {
        sprite_index = spriteMouth;
        x += image_xscale*mouthPosX;
        y += image_yscale*mouthPosYinit;
        mouthPosY = mouthPosYinit;
        canFillHealthBar = false;
        startIntro = false;
        isIntro = true;
        visible = true;
        grav = gravStart;
        image_speed = 0.125;
        timer = 0;
        image_alpha = 0;
        myHurtbox = instance_create(xstart,ystart,objWilyMachineGBIHurtbox);
        myHurtbox.motherObject = self;
        myHurtbox.image_alpha = 0;
        canDamage = false;
    }
    else if (isIntro)
    {
        // custom intro:
        if (image_alpha &lt; 1)
        {
            image_alpha += 1/65;
            myHurtbox.image_alpha += 1/65;
        }
        else
        {
            image_alpha = 1;
            myHurtbox.image_alpha = 1;
            if (!hasTriggeredFall)
            {
                hasTriggeredFall = true;
            }
            
            with(myHurtbox)
            {
                sprite_index = other.spriteIntro;
                imgSpd = 0.25;
            }
            timer = 0;
            phase = 0;
            canFillHealthBar = true;
            isIntro = false;
            blockCollision = blockCollisionStart;
            canDamage = true;
            projectileTracker = 0;
        }
    }
}
if (entityCanStep())
{
    if(isFight)
    {
        if(phase == 0) // wait
        {
            timer++;
            if(timer &gt; mouthUptime)
            {
                timer = 0;
                phase++;
            }
        }
        else if(phase == 1) // open mouth
        {
            if(mouthPosY &lt; mouthPosYmax) mouthPosY += mouthSpd;
            else // attack
            {
                mouthPosY = mouthPosYmax;
                projectileTracker = !projectileTracker;
                var wheel = instance_create(x,y-8,objWilyMachineGBICuttingWheel);
                wheel.image_xscale = image_xscale;
                wheel.xspeed = image_xscale*1.5;
                wheel.yspeed = -2.5;
                var launchDist = 64;
                if(target)
                {
                    launchDist = round(abs(x - target.x)/64)*64;
                    if(launchDist &lt; 64) launchDist = 64;
                }
                if(projectileTracker)
                {
                    wheel.bounces = 1;
                    wheel.yspeed = -5.5 + choose(0,-1);
                    var launchPos = x+launchDist*image_xscale;
                    with(wheel) xspeed = xSpeedAim(x,y,launchPos,y,yspeed,grav);
                }
                phase++;
            }
        }
        else if(phase == 2) // wait again
        {
            timer++;
            if(timer &gt; mouthDowntime)
            {
                timer = 0;
                phase++;
            }
        }
        else if(phase == 3) // close mouth
        {
            if(mouthPosY &gt; mouthPosYinit) mouthPosY -= mouthSpd;
            else
            {
                mouthPosY = mouthPosYinit;
                phase = 0;
            }
        }
        
        y = mouthPosY + ystart;
    }
}
