                        mashGetoutCooldownTimer = mashGetoutCooldown;
                    }
                }
                else
                {
                    mashGetoutCooldownTimer--;
                    if((mashGetoutCooldownTimer mod 4) &lt; 2) drawOffsetY = -image_yscale;
