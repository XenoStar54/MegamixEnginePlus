            <kind>1</kind>
            <string>// grab stage info
if (instance_exists(prtBoss) || instance_exists(objEnergyElement))
{
    var ender = instance_find(prtBoss, 0);
    if (instance_exists(ender))
    {
        elementName = ender.elementName;
        elementCode = ender.elementCode;
        elementScript = ender.elementScript;
    }
    else
    {
        ender = instance_find(objEnergyElement, 0);
        elementName = ender.name;
        elementCode = ender.code;
        elementScript = ender.script;
    }
    
    check = instance_nearest(ender.x, ender.y, objCheckpoint);
    checkTouch = instance_nearest(ender.x, ender.y, objCheckpointTouch);
    
    if (instance_exists(checkTouch))
    {
        if (instance_exists(check))
        {
            if (point_distance(ender.x, ender.y, checkTouch.x, checkTouch.y) 
                &lt; point_distance(ender.x, ender.y, ender.x, ender.y))
            {
                check = checkTouch;
            }
        }
        else
        {
            check = checkTouch;
        }
    }
    
    if (instance_exists(check))
    {
        bossCheckpointX = check.xx;
        bossCheckpointY = check.yy;
    }
}
else
{
    elementName = "";
    elementCode = "";
    elementScript = scrNoEffect;
    bossTeleportX = -1;
    bossTeleportY = -1;
}
