        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!dead)
{
    var _x = (floor((x + drawOffsetX * global.screenScale) + .5) / global.screenScale);
    var _y = (floor((y + drawOffsetY * global.screenScale) + .5) / global.screenScale);
    if(iFrames &gt; 0) d3d_set_fog(true, c_white, 0, 0);
