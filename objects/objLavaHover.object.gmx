        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// swims over lava and carries entities through
event_inherited();

blockCollision = 1;
grav = 0.25;
saveGrav = 0.25;
isSolid = 2;
canHit = false;
canDamage = false;
itemDrop = -1;
shiftVisible = 3;
stopOnFlash = false;

maxShiftDistance = 8;
lavaRiseSpd = 4;

hoverOnLava = 1;
hoverOnWater = 1;
hoverOnAir = 1;
hoverOnGround = 0;

freeFlySpd = 2;

myPassenger = noone;

passengerLoseTimerMax = 120;
passengerLoseTimer = 0;

passengerBlacklist[0] = objPhysicsBlock;

elemSprite[0] = sprLavaHover;
elemSprite[1] = sprLavaHoverAlt1;
elemSprite[2] = sprLavaHoverAlt2;
elemSprite[3] = sprLavaHoverAlt3;

elemSelector = 0;

imgSpd = 0.3;
imgIndex = 0;

baseImg = 0;
eyeImg = 2;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>event_inherited();

if(entityCanStep())
{
    var newElemSelector = elemSelector;
    // rise in lava
    var rise = false, standOnLava = false, freeFlyMode = false;
    if(hoverOnLava)
    {
        with(objLava)
        {
            if(!dead &amp;&amp; iceTimer &lt;= 0)
            {
                if(place_meeting(x,y+5*sign(other.saveGrav),other))
                {
                    rise = true;
                    newElemSelector = 3;
                }
                else if(place_meeting(x,y+4*sign(other.saveGrav),other))
                {
                    standOnLava = true;
                    newElemSelector = 3;
                }
            }
        }
    }
    // rise in water
    if(hoverOnWater)
    {
        with(objWater)
        {
            if(place_meeting(x,y+5*sign(other.saveGrav),other))
            {
                rise = true;
                newElemSelector = 0;
            }
            else if(place_meeting(x,y+4*sign(other.saveGrav),other))
            {
                standOnLava = true;
                newElemSelector = 0;
            }
        }
    }
    // rise when touching wind currents
    if(hoverOnAir)
    {
        if(instance_exists(objWindRight) &amp;&amp; place_meeting(x,y,objWindRight))
        {
            freeFlyMode = true;
            newElemSelector = 2;
        }
        if(instance_exists(objTenguWind) &amp;&amp; objTenguWind.activated &amp;&amp; objTenguWind.windSpeed != 0)
        {
            freeFlyMode = true;
            newElemSelector = 2;
        }
        if(!freeFlyMode) with(objFanPush)
        {
            if(place_meeting(x,y+5*sign(other.saveGrav),other))
            {
                rise = true;
                newElemSelector = 2;
            }
            else if(place_meeting(x,y+4*sign(other.saveGrav),other))
            {
                standOnLava = true;
                newElemSelector = 2;
            }
        }
    }
    // rise when touching level geometry and object floor lmao
    if(hoverOnGround &amp;&amp; !blockCollision)
    {
        if(checkSolid(0,-5*sign(saveGrav),1,1))
        {
            rise = true;
            newElemSelector = 1;
        }
        else if(checkSolid(0,-4*sign(saveGrav),1,1))
        {
            standOnLava = true;
            newElemSelector = 1;
        }
    }
    
    if(newElemSelector != elemSelector)
    {
        iFrames = 4;
        elemSelector = newElemSelector;
    }
    sprite_index = elemSprite[elemSelector];
    
    baseImg = 0;
    eyeImg = 5;
    imgIndex += imgSpd;
    
    if(freeFlyMode)
    {
        grav = 0;
        xspeed = 0;
        yspeed = 0;
    }
    else if(rise)
    {
        xspeed = 0;
        grav = 0;
        yspeed = -sign(saveGrav)*lavaRiseSpd;
        eyeImg = 7;
    }
    else if(standOnLava)
    {
        xspeed = 0;
        grav = 0;
        yspeed = 0;
    }
    else
    {
        xspeed = 0;
        grav = saveGrav;
        if(sign(yspeed) == -sign(saveGrav)) yspeed = 0;
        eyeImg = 3 + 2*ground;
    }
    
    if(myPassenger == noone) // find passenger
    {
        baseImg = 0;
        eyeImg = 2;
        shiftVisible = 0;
        with(prtEntity) if(!dead &amp;&amp; blockCollision)
        {
            var valid = true;
            with(other)
            {
                for(var i = 0; i &lt; array_length_1d(passengerBlacklist); i++)
                {
                    if(passengerBlacklist[i] == other) valid = false;
                }
            }
            
            if(valid &amp;&amp; ground &amp;&amp; place_meeting(x,y+sign(grav),other) &amp;&amp; !place_meeting(x,y,other))
            {
                other.myPassenger = self;
                other.passengerLoseTimer = -5;
            }
        }
    }
    else // I have passenger
    {
        shiftVisible = 3;
        if(!instance_exists(myPassenger) || myPassenger.dead || myPassenger == noone) myPassenger = noone;
        else
        {
            // follow/carry passenger
            if(!ground &amp;&amp; passengerLoseTimer &gt;= 0)
            {
                if(freeFlyMode)
                {
                    if(myPassenger.object_index == objMegaman)
                    {
                        var angle = point_direction(0,0,myPassenger.xDir,myPassenger.yDir);
                        if(myPassenger.xDir != 0 || myPassenger.yDir != 0)
                        {
                            xspeed = cos(degtorad(angle))*freeFlySpd;
                            yspeed = -sin(degtorad(angle))*freeFlySpd;
                        }
                        if(abs(xspeed) &gt; abs(yspeed)) eyeImg = 5+image_xscale*sign(xspeed);
                        else eyeImg = 5-2*sign(xspeed);
                    }
                }
                var dist = (bboxGetXCenterObject(myPassenger) - x);
                if(abs(dist) &gt; maxShiftDistance) dist = sign(dist)*maxShiftDistance;
                // real
                if(!(checkSolid(dist,0) &amp;&amp; blockCollision))
                {
                    shiftObject(dist,0,blockCollision);
                    var newX = x;
                    shiftObject(-dist,0,blockCollision);
                    x = newX;
                }
                if(eyeImg == 5) eyeImg = 5+image_xscale*sign(dist);
            }
            
            // disown passenger if they broke contact
            if(myPassenger.ground &amp;&amp; place_meeting(x,y-4*sign(myPassenger.grav),myPassenger) &amp;&amp; !place_meeting(x,y,myPassenger))
            {
                if(passengerLoseTimer &lt; 0) passengerLoseTimer++;
                else passengerLoseTimer = 0;
                baseImg = 1;
            }
            else // I've been abandoned
            {
                if(myPassenger.ground)
                {
                    passengerLoseTimer++;
                    var closeTimer = passengerLoseTimerMax-passengerLoseTimer;
                    if(closeTimer &lt; 60)
                    {
                        if((closeTimer mod 20) == 19) playSFX(sfxBladeManSeeSawCart2);
                        if((closeTimer mod 20) &gt;= 10) iFrames = 4;
                        else iFrames = 0;
                    }
                    if(passengerLoseTimer &gt; passengerLoseTimerMax)
                    {
                        myPassenger = noone;
                    }
                }
            }
        }
    }
    
    if(passengerLoseTimer &lt; 0)
    {
        baseImg = 1;
        eyeImg = 3;
        drawOffsetY = 2;
    }
    else drawOffsetY = 0;
}
</string>
