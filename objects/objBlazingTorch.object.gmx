                    i.depth -= flames;
                    if (flames == 3){
                        i.sprite_index = sprBlazingTorchDragon;
                    }
                    else i.sprite_index = sprBlazingTorch;
                    flames--;
                }
                if (flames &lt;= 0) event_user(EV_DEATH);
            }
        }
    }


