                if (timer mod flameMod == 0 &amp;&amp; flames &gt; 0){
                    a = instance_create(x, y, objSingleLoopEffect);
                    a.sprite_index = sprBlazingTorchSpawn;
                    i = instance_create(x, y, objBlazingTorch);
